{
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jasmine": true
  },
  "rules": {
    "no-plusplus": "off",
    "arrow-body-style": 0,
    "no-param-reassign": 0
  }
}